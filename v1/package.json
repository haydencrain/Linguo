{
  "name": "linguo",
  "version": "1.0.0",
  "description": "Simpsons themed Discord bot using discord.js",
  "main": "app.js",
  "scripts": {
    "start-prod": "npm run clean && npm run build && npm run start-build-prod",
    "clean": "rm -rf build && mkdir build",
    "build": "npx babel -d ./build --ignore node_modules",
    "start-build-prod": "ENVIRONMENT='production' node ./build/app.js",
    "start": "ENVIRONMENT='development' babel-node app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "enginges": {
    "node": ">=8",
    "npm": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haydencrain/Linguo"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chalk": "^2.4.1",
    "discord.js": "^11.3.0",
    "opusscript": "0.0.6",
    "query-string": "^6.0.0",
    "raven": "^2.6.0",
    "ytdl-core": "^0.25.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.18.1"
  },
  "author": "haydencrain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haydencrain/Linguo/issues"
  }
}
